#include<iostream>
using namespace std;    
int main(){
    int size;
    cout<<"Enter the size: ";
    cin>>size;

    int arr[size];
    for(int i=0;i<size;i++){
        cin>>arr[i];
    }

    for(int pass=0;pass<size;pass++){
        int key=arr[pass];
        int ele=pass-1;
        while(ele>=0 && key<arr[ele]){
            arr[ele+1]=arr[ele];
            ele--;
        }
        arr[ele+1]=key;
    }
     
        for(int i=0;i<size;i++){
            cout<<arr[i]<<" ";
        }
    return 0;
}
