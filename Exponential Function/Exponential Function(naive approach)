//Exponential function using divide and conquer
#include <iostream>
using namespace std;

//Exponential Function using Iterative (Naive) Approach
long long expoIterative(long long x, int n) {
    long long result = 1;
    for (int i = 0; i < n; i++) {
        result *= x;
    }
    return result;
}

int main() {
    cout << expoIterative(2, 10);
}

